<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="com.capita.MohammadDemo" version="1.2" xmlns="http://www.w3.org/ns/widgets" defaultlocale="en-US">
  <name>Mohammad Demo</name>
  <description>Demo</description>
  <author href="http://www.capita-software.co.uk" email="engagesupport@capita.co.uk">Capita Business Services Ltd.</author>
  <content src="index.html"/>
  <icon src="icon.png"/>
  <splash src="splash.png"/>
  <preference name="target-device" value="universal"/>
  <preference name="KeepRunning" value="True"/>
  <preference name="ShowTitle" value="False"/>
  <preference name="InAppBrowserStorageEnabled" value="True"/>
  <preference name="SuppressesIncrementalRendering" value="True"/>
  <preference name="Orientation" value="portrait"/>
  <preference name="StatusBarOverlaysWebView" value="false"/>
  <preference name="KeyboardShrinksView" value="false"/>
  <preference name="DisallowOverscroll" value="true"/>
  <preference name="BackupWebStorage" value="none"/>
  <preference name="Fullscreen" value="false"/>
  <preference name="android-windowSoftInputMode" value="adjustResize"/>
  <preference name="deployment-target" value=""/>
  <preference name="SplashScreen" value="splash"/>
  <preference name="SplashScreenDelay" value="60000"/>
  <preference name="ShowSplashScreenSpinner" value="true"/>
  <preference name="auto-hide-splash-screen" value="False"/>
  <preference name="KeyboardDisplayRequiresUserAction" value="false"/>
  <allow-intent href="http:*"/>
  <allow-intent href="https:*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <allow-navigation href="*"/>
    <preference name="android-minSdkVersion" value="14"/>
    <config-file platform="android" parent="/manifest" mode="merge">
      <application android:theme="@android:style/Theme.NoTitleBar"/>
      <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
      <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
      <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS"/>
    </config-file>
    <splash qualifier="land-hdpi" src="res/screens/android/drawable-land-hdpi/splash.png"/>
    <splash qualifier="land-ldpi" src="res/screens/android/drawable-land-ldpi/splash.png"/>
    <splash qualifier="land-mdpi" src="res/screens/android/drawable-land-mdpi/splash.png"/>
    <splash qualifier="land-xhdpi" src="res/screens/android/drawable-land-xhdpi/splash.png"/>
    <splash qualifier="land-xxhdpi" src="res/screens/android/drawable-land-xxhdpi/splash.png"/>
    <splash qualifier="land-xxxhdpi" src="res/screens/android/drawable-land-xxxhdpi/splash.png"/>
    <splash qualifier="port-hdpi" src="res/screens/android/drawable-port-hdpi/splash.png"/>
    <splash qualifier="port-ldpi" src="res/screens/android/drawable-port-ldpi/splash.png"/>
    <splash qualifier="port-mdpi" src="res/screens/android/drawable-port-mdpi/splash.png"/>
    <splash qualifier="port-xhdpi" src="res/screens/android/drawable-port-xhdpi/splash.png"/>
    <splash qualifier="port-xxhdpi" src="res/screens/android/drawable-port-xxhdpi/splash.png"/>
    <splash qualifier="port-xxxhdpi" src="res/screens/android/drawable-port-xxxhdpi/splash.png"/>
  </platform>
  <platform name="ios">
    <preference name="phonegap-version" value="cli-6.4.0"/>
    <preference name="KeyboardShrinksView" value="true"/>
    <preference name="StatusBarOverlaysWebView" value="false"/>
    <preference name="StatusBarBackgroundColor" value="#000000"/>
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <splash src="res/screens/ios/screen-iphone-portrait.png" width="320" height="480"/>
    <splash src="res/screens/ios/screen-iphone-portrait-2x.png" width="640" height="960"/>
    <splash src="res/screens/ios/screen-ipad-portrait.png" width="768" height="1024"/>
    <splash src="res/screens/ios/screen-ipad-portrait-2x.png" width="1536" height="2048"/>
    <splash src="res/screens/ios/screen-ipad-landscape.png" width="1024" height="768"/>
    <splash src="res/screens/ios/screen-ipad-landscape-2x.png" width="2048" height="1536"/>
    <splash src="res/screens/ios/screen-iphone-568h-2x.png" width="640" height="1136"/>
    <splash src="res/screens/ios/screen-iphone-portrait-667h.png" width="750" height="1334"/>
    <splash src="res/screens/ios/screen-iphone-portrait-736h.png" width="1242" height="2208"/>
    <splash src="res/screens/ios/screen-iphone-landscape-736h.png" width="2208" height="1242"/>
    <icon src="res/icons/ios/icon180x180.png" width="180" height="180"/>
    <icon src="res/icons/ios/icon60x60.png" width="60" height="60"/>
    <icon src="res/icons/ios/icon40x40.png" width="40" height="40"/>
    <icon src="res/icons/ios/icon80x80.png" width="80" height="80"/>
    <icon src="res/icons/ios/icon57x57.png" width="57" height="57"/>
    <icon src="res/icons/ios/icon114x114.png" width="114" height="114"/>
    <icon src="res/icons/ios/icon72x72.png" width="72" height="72"/>
    <icon src="res/icons/ios/icon144x144.png" width="144" height="144"/>
    <icon src="res/icons/ios/icon29x29.png" width="29" height="29"/>
    <icon src="res/icons/ios/58x58icon.png" width="58" height="58"/>
    <icon src="res/icons/ios/icon50x50.png" width="50" height="50"/>
    <icon src="res/icons/ios/icon100x100.png" width="100" height="100"/>
    <icon src="res/icons/ios/icon120x120.png" width="120" height="120"/>
    <icon src="res/icons/ios/icon152x152.png" width="152" height="152"/>
    <icon src="res/icons/ios/icon76x76.png" width="76" height="76"/>
  </platform>
  <plugin name="cordova-plugin-background-app"/>
  <plugin name="cordova-plugin-file"/>
  <plugin name="cordova-plugin-calendar"/>
  <plugin name="cordova-plugin-file-transfer"/>
  <plugin name="cordova-plugin-pin-dialog"/>
  <plugin name="cordova-plugin-splashscreen"/>
  <plugin name="cordova-plugin-inappbrowser"/>
  <plugin name="phonegap-plugin-contentsync" version="1.2.3" src="https://github.com/phonegap/phonegap-plugin-contentsync"/>
  <plugin name="cordova-plugin-network-information"/>
  <plugin name="de.appplant.cordova.plugin.local-notification"/>
  <plugin name="cordova-plugin-device"/>
  <plugin name="cordova-plugin-geolocation"/>
  <plugin name="cordova-plugin-backbutton"/>
  <plugin name="cordova-plugin-secure-storage"/>
  <plugin name="cordova-plugin-nativecamera" spec="~1.4.2"/>
  <plugin name="cordova-plugin-compat" spec="~1.1.0"/>
  <plugin name="cordova-plugin-camera"/>
  <plugin name="cordova-plugin-console" spec="~1.0.3"/>
  <plugin name="cordova-plugin-dialogs" spec="~1.2.1"/>
  <plugin name="cordova-plugin-statusbar" spec="~2.1.3"/>
  <plugin name="phonegap-plugin-push" spec="~1.8.0">
    <variable name="SENDER_ID" value="XXXXXXX"/>
  </plugin>
  <plugin name="cordova-plugin-whitelist"/>
  <config-file platform="ios" target="*-Info.plist" parent="NSCalendarsUsageDescription">
    <string>Some ad content may access calendar</string>
  </config-file>
  <config-file platform="ios" target="*-Info.plist" parent="NSAppTransportSecurity">
    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <true/>
    </dict>
  </config-file>
  <config-file platform="ios" target="*-Info.plist" parent="NSPhotoLibraryUsageDescription">
    <string>Some ad content may access photo</string>
  </config-file>
  <config-file platform="ios" target="*-Info.plist" parent="NSCameraUsageDescription">
    <string>Some ad content may access camera</string>
  </config-file>
  <access origin="http://*"/>
  <access origin="https://*"/>
  <access origin="tel:*" launch-external="yes"/>
  <access origin="geo:*" launch-external="yes"/>
  <access origin="mailto:*" launch-external="yes"/>
  <access origin="sms:*" launch-external="yes"/>
  <access origin="market:*" launch-external="yes"/>
  <access origin="https://twitter.com" subdomains="true"/>
  <access origin="http://twitter.com" subdomains="true"/>
  <access origin="http://platform.twitter.com" subdomains="true"/>
  <access origin="https://platform.twitter.com" subdomains="true"/>
  <access origin="https://cdn.syndication.twimg.com"/>
  <access origin="http://cdn.syndication.twimg.com"/>
  <access origin="https://www.facebook.com" subdomains="true"/>
  <access origin="http://www.facebook.com" subdomains="true"/>
  <access origin="http://www.connect.facebook.net" subdomains="true"/>
  <access origin="http://m.facebook.com" subdomains="true"/>
  <allow-navigation href="about:*"/>
  <allow-navigation href="https://www.facebook.com"/>
  <allow-navigation href="http://www.facebook.com"/>
  <allow-navigation href="http://www.connect.facebook.net"/>
  <feature name="IntentAndNavigationFilter">
    <param name="ios-package" value="CDVIntentAndNavigationFilter"/>
    <param name="onload" value="true"/>
  </feature>
  <version>1.2</version>
</widget>